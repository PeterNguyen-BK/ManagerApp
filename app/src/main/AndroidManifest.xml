<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.managerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Manager.SupplierList" />
        <activity android:name=".Manager.ManagerHomePage" />

        <service
            android:name=".Supplier.Service.ComingOrder"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".Manager.NewSupplier" />
        <activity android:name=".Manager.ManagerLogin" />
        <activity android:name=".Supplier.Login.LoginPage" />
        <activity android:name=".Supplier.EditFood.EditFoodActivity" />

        <activity
            android:name=".Supplier.HomePage"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>